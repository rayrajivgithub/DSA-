#include <iostream>
using namespace std;
void bubble(int arr[],int n){
   
    for(int i=0;i<n-1;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]>arr[j]){
                swap(arr[i],arr[j]);
            }
        }
        
    }
    for(int k=0;k<n;k++){
        cout<<arr[k]<< " ";
    }
}

int main() {
    int arr[10]={0,10,9,12,141,120,121,8,1,2};
    bubble(arr,10);

    return 0;
}
